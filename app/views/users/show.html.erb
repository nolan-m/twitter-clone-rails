<% if current_user == @user %>
  <%= form_for @tweet do |t| %>
    <%= t.label :content, "TWEET" %>
  </br>
    <%= t.text_field :content %>
  </br>
    <%= t.submit %>
  <% end %>
<% end %>

<ul>
  <% @user.tweets.each do |t| %>
   <li> <%= t.content %> | tweeted <%= time_ago_in_words(t.created_at) %> ago.</li>
  <% end %>
</ul>

